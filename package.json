{
  "name": "apprenderbackend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/src/server.js",
    "prebuild": "npm test && rimraf build",
    "build": "npx tsc -b",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "test": "jest --colors",
    "prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodcordeiro/ApprenderBackend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rodcordeiro/ApprenderBackend/issues"
  },
  "homepage": "https://github.com/rodcordeiro/ApprenderBackend#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.4.0",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.0.0",
    "pg": "^8.5.1",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "2.8.12",
    "@types/crypto-js": "4.1.1",
    "@types/express": "4.17.13",
    "@types/helmet": "4.0.0",
    "@types/jest": "28.1.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/morgan": "1.9.3",
    "@types/node": "17.0.35",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/parser": "5.27.0",
    "cross-env": "7.0.3",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.1.0",
    "morgan": "1.10.0",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "sqlite3": "5.0.8",
    "supertest": "6.2.3",
    "ts-jest": "28.0.4",
    "ts-node": "10.8.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.7.3"
  },
  "renovate": {
    "branchPrefix": "updates/",
    "packageRules": [
      {
        "updateTypes": [
          "minor",
          "patch",
          "pin",
          "digest"
        ],
        "automerge": true
      },
      {
        "updateTypes": [
          "major"
        ],
        "automerge": false
      }
    ],
    "assignAutomerge": true,
    "assignees": [
      "rodcordeiro",
      "JuniorAlvess"
    ],
    "labels": [
      "dependencies"
    ],
    "recreateClosed": true,
    "rebaseWhen": "conflicted",
    "vulnerabilityAlerts": {
      "labels": [
        "security"
      ],
      "assignees": [
        "@rodcordeiro"
      ]
    },
    "extends": [
      "config:base",
      ":dependencyDashboard",
      "github>whitesource/merge-confidence:beta"
    ]
  },
  "jest": {
    "clearMocks": true,
    "rootDir": "./__tests__",
    "testRegex": ".*\\.test\\.(t|j)s$",
    "testEnvironmentOptions": {
      "NODE_ENV": "test",
      "PORT": 3333
    },
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageDirectory": "./__coverage__",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ]
  }
}
